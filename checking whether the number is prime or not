#include <stdio.h>

int main()
{
	int t,i,n,m;
	scanf("%d",&t);
	for(i=0;i<t;i++)
	{
	    int k=1;
	    scanf("%d",&n);
	    m=n/2;
	    if(n==0 || n==1)
	    k=0;
	    for(int i=2;i<m;i++)
	    {
	        if(n%i==0)
	        {
	        k=0;
	        break;
	            
	        }
	    }
	    if(k==0)
	    printf("no\n");
	    else
	    printf("yes\n");
	}
	return 0;
}

