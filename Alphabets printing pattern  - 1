#include <stdio.h>
int main()
{
   int i,j;
   char x='A';
   j=1;
   while(j<=5)
   {
       i=1;
       while(i<=6-j)
       {
         printf(" ");
         i=i+1;
       }
      
       i=1;
       while(i<=j)
       {
           printf("%c ",x);
           i=i+1;
            x=x+1;
       }
     printf("\n");
     
     j=j+1;
   }
}
